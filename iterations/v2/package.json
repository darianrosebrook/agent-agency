{
  "name": "agent-agency-v2",
  "version": "2.0.0",
  "description": "Agent Agency V2 - Agentic RL & Extended Thinking with production-grade reliability and safety measures",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest --maxWorkers=1",
    "test:safe": "jest --maxWorkers=1 --testTimeout=10000",
    "test:unit": "jest --testPathPattern=unit --maxWorkers=1",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:e2e:db": "./scripts/run-e2e-tests.sh",
    "test:contract": "jest --testPathPattern=contract",
    "test:mutation": "stryker run",
    "test:coverage": "jest --coverage --maxWorkers=1",
    "test:coverage:full": "jest --coverage --maxWorkers=4",
    "test:rl": "jest --testPathPattern=rl",
    "test:thinking": "jest --testPathPattern=thinking",
    "test:evaluation": "jest --testPathPattern=evaluation",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "validate-verdict": "node scripts/validate-verdict.cjs",
    "benchmark:agent-registry": "tsx benchmarks/agent-registry-performance.ts",
    "db:setup": "node scripts/setup-test-db.cjs",
    "dev": "tsx watch src/index.ts",
    "dev:mcp": "tsx watch src/mcp/server.ts",
    "start": "node dist/src/index.js",
    "start:mcp": "node dist/src/mcp/server.js",
    "clean": "rm -rf dist coverage .stryker-tmp",
    "cleanup:test-processes": "./scripts/cleanup-test-processes.sh",
    "mcp:start": "node bin/mcp-server.js start",
    "validate": "caws validate --suggestions",
    "db:migrate": "node scripts/migrate-db.js up",
    "db:migrate:status": "node scripts/migrate-db.js status",
    "db:rollback": "node scripts/migrate-db.js down",
    "db:seed": "node scripts/seed-db.js",
    "perf:budgets": "node scripts/check-perf-budgets.js",
    "thinking:budgets": "node scripts/analyze-thinking-budgets.js",
    "rl:train": "node scripts/train-rl-models.js",
    "rl:eval": "node scripts/evaluate-rl-models.js",
    "rl:batch": "node scripts/batch-rl-training.js",
    "minimal-diff:analyze": "node scripts/analyze-minimal-diff.js",
    "tool-learning:eval": "node scripts/evaluate-tool-learning.js",
    "benchmark:v2": "node scripts/benchmark-v2-features.js",
    "ab:test": "node scripts/run-ab-tests.js",
    "index:knowledge": "tsx scripts/index-knowledge.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darianrosebrook/agent-agency.git"
  },
  "keywords": [
    "agent",
    "agency",
    "ai",
    "orchestration",
    "multi-agent",
    "mcp",
    "agentic-rl",
    "reinforcement-learning",
    "thinking-budgets",
    "minimal-diff-evaluation",
    "tool-learning",
    "extended-thinking",
    "reward-hacking-prevention",
    "production-grade"
  ],
  "author": "@darianrosebrook",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/jest-runner": "^8.0.0",
    "@stryker-mutator/typescript-checker": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "openapi-types": "^12.1.3",
    "testcontainers": "^10.0.0",
    "ts-jest": "^29.4.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@caws/mcp-server": "file:../../../caws/packages/caws-mcp-server",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@paths.design/caws-cli": "file:../../../caws/packages/caws-cli",
    "@types/cheerio": "^0.22.35",
    "@types/chokidar": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@typescript/vfs": "^1.5.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "ast-types": "^0.14.2",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "chokidar": "^3.6.0",
    "differential-privacy": "^1.0.0",
    "dotenv": "^16.3.1",
    "fast-check": "^3.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^15.0.0",
    "ollama": "^0.3.0",
    "openai": "^4.24.7",
    "pg": "^8.16.3",
    "recast": "^0.23.4",
    "redis": "^5.8.3",
    "robots-parser": "^3.0.1",
    "tree-sitter": "^0.20.4",
    "tree-sitter-typescript": "^0.20.3",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "bin": {
    "mcp-server": "./bin/mcp-server.js",
    "agent-agency": "./bin/agent-agency.js"
  },
  "engines": {
    "node": ">=20.5.0"
  },
  "caws": {
    "tier": 2,
    "features": [
      "thinking_budgets",
      "minimal_diff_evaluation",
      "turn_level_rl",
      "model_based_judges",
      "tool_learning"
    ],
    "contracts": [
      "docs/api/v2-rl-training.yaml",
      "docs/api/v2-thinking-budget.yaml",
      "docs/api/v2-evaluation-enhanced.yaml"
    ]
  }
}
