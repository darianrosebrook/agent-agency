{
  "dashboard": {
    "id": null,
    "title": "Multimodal RAG - SLO Tracking",
    "tags": ["multimodal-rag", "slo", "reliability"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "SLO Status Overview",
        "type": "table",
        "targets": [
          {
            "expr": "multimodal_slo_status",
            "refId": "A",
            "legendFormat": "{{slo_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Compliant",
                    "color": "green"
                  },
                  "1": {
                    "text": "At Risk",
                    "color": "orange"
                  },
                  "2": {
                    "text": "Violated",
                    "color": "red"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Availability SLO",
        "type": "graph",
        "targets": [
          {
            "expr": "multimodal_slo_api_availability",
            "refId": "A",
            "legendFormat": "Availability %"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 95,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 99.5,
            "colorMode": "critical",
            "op": "lt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "Task Completion SLO",
        "type": "graph",
        "targets": [
          {
            "expr": "multimodal_slo_task_completion",
            "refId": "A",
            "legendFormat": "Completion Rate %"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 95,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 98,
            "colorMode": "critical",
            "op": "lt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Response Time SLOs",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Council Decision Time",
        "type": "graph",
        "targets": [
          {
            "expr": "multimodal_slo_council_decision_time",
            "refId": "A",
            "legendFormat": "P95 Decision Time (ms)"
          }
        ],
        "yAxes": [
          {
            "unit": "milliseconds",
            "max": 10000
          }
        ],
        "thresholds": [
          {
            "value": 5000,
            "colorMode": "warning",
            "op": "gt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 6,
        "title": "Worker Execution Time",
        "type": "graph",
        "targets": [
          {
            "expr": "multimodal_slo_worker_execution_time",
            "refId": "A",
            "legendFormat": "P95 Execution Time (ms)"
          }
        ],
        "yAxes": [
          {
            "unit": "milliseconds",
            "max": 60000
          }
        ],
        "thresholds": [
          {
            "value": 30000,
            "colorMode": "warning",
            "op": "gt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 17
        }
      },
      {
        "id": 7,
        "title": "SLO Alerts",
        "type": "table",
        "targets": [
          {
            "expr": "multimodal_slo_alerts",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 25
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5m",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
