{
  "name": "agent-agency-v3",
  "version": "0.1.0",
  "description": "Agent Agency V3 - Enterprise-grade AI agent orchestration system",
  "private": true,
  "scripts": {
    "test": "cargo test --workspace --all-features",
    "test:unit": "cargo test --workspace --lib --all-features",
    "test:integration": "cargo test --workspace --test '*' --all-features -- --test-threads=1",
    "test:contract": "cargo test --workspace contract --all-features",
    "test:e2e": "cargo test --workspace e2e --all-features",
    "test:coverage": "make test-coverage",
    "coverage:report": "make coverage-report",
    "schema:validate": "make schema-validate",
    "verify": "./scripts/verify.sh",
    "verify:tier1": "TIER=1 COVERAGE_MIN=0.90 MUTATION_MIN=0.70 ENABLE_MUTATION=true ENABLE_CONTRACT=true ENABLE_E2E=true ./scripts/verify.sh",
    "verify:tier2": "TIER=2 COVERAGE_MIN=0.80 MUTATION_MIN=0.50 ENABLE_MUTATION=true ENABLE_CONTRACT=true ./scripts/verify.sh",
    "verify:tier3": "TIER=3 COVERAGE_MIN=0.70 MUTATION_MIN=0.30 ./scripts/verify.sh",
    "lint": "cargo clippy --workspace --all-features -- -D warnings",
    "fmt": "cargo fmt --all",
    "fmt:check": "cargo fmt --all --check",
    "build": "cargo build --workspace --all-features",
    "build:release": "cargo build --workspace --release",
    "install-hooks": "make install-hooks",
    "todo-check": "python3 scripts/todo_analyzer.py",
    "clean": "make clean"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "js-yaml": "^4.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

