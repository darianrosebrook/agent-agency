{
  "name": "agent-agency",
  "version": "0.1.0",
  "description": "Intelligent Multi-Agent Orchestration Platform - Mono-repo",
  "private": true,
  "type": "module",
  "workspaces": [
    "iterations/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build": "npm run --workspace=iterations/poc build",
    "build:poc": "npm run --workspace=iterations/poc build",
    "test": "npm run --workspace=iterations/poc test",
    "test:poc": "npm run --workspace=iterations/poc test",
    "test:all": "npm run --workspace=iterations/poc test",
    "lint": "npm run --workspace=iterations/poc lint",
    "lint:poc": "npm run --workspace=iterations/poc lint",
    "typecheck": "npm run --workspace=iterations/poc typecheck",
    "typecheck:poc": "npm run --workspace=iterations/poc typecheck",
    "dev": "npm run --workspace=iterations/poc dev",
    "dev:poc": "npm run --workspace=iterations/poc dev",
    "clean": "npm run --workspace=iterations/poc clean",
    "migrate": "npm run --workspace=iterations/poc migrate",
    "migrate:poc": "npm run --workspace=iterations/poc migrate",
    "iterations:list": "node scripts/manage-iterations.js list",
    "iterations:validate": "node scripts/manage-iterations.js validate",
    "iterations:stats": "node scripts/manage-iterations.js stats",
    "iterations:run": "node scripts/manage-iterations.js run",
    "iterations:run-all": "node scripts/manage-iterations.js run-all",
    "caws:validate": "node scripts/caws-validate.js",
    "caws:validate:all": "node scripts/caws-validate.js all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/agent-agency.git"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "multi-agent",
    "mcp",
    "typescript"
  ],
  "author": "@darianrosebrook",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/agent-agency/issues"
  },
  "homepage": "https://github.com/your-org/agent-agency#readme",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
