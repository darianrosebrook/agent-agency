{
  "name": "agent-agency",
  "version": "0.1.0",
  "description": "Intelligent Multi-Agent Orchestration Platform - Mono-repo",
  "private": true,
  "type": "module",
  "workspaces": [
    "iterations/*"
  ],
  "scripts": {
    "install:all": "pnpm install",
    "build": "pnpm --filter iterations/poc build",
    "build:poc": "pnpm --filter iterations/poc build",
    "build:v2": "pnpm --filter iterations/v2 build",
    "build:v3": "pnpm --filter iterations/v3 build",
    "build:all": "turbo build",
    "test": "echo 'POC tests disabled - focusing on CAWS extraction'",
    "test:poc": "pnpm --filter iterations/poc test",
    "test:v2": "pnpm --filter iterations/v2 test",
    "test:v3": "pnpm --filter iterations/v3 test",
    "test:all": "turbo test",
    "lint": "pnpm --filter iterations/poc lint",
    "lint:poc": "pnpm --filter iterations/poc lint",
    "lint:v2": "pnpm --filter iterations/v2 lint",
    "lint:v3": "pnpm --filter iterations/v3 lint",
    "lint:all": "turbo lint",
    "typecheck": "pnpm --filter iterations/poc typecheck",
    "typecheck:poc": "pnpm --filter iterations/poc typecheck",
    "typecheck:v2": "pnpm --filter iterations/v2 typecheck",
    "typecheck:v3": "pnpm --filter iterations/v3 typecheck",
    "typecheck:all": "turbo typecheck",
    "dev": "pnpm --filter iterations/poc dev",
    "dev:poc": "pnpm --filter iterations/poc dev",
    "dev:v2": "pnpm --filter iterations/v2 dev",
    "dev:v3": "pnpm --filter iterations/v3 dev",
    "clean": "pnpm --parallel clean",
    "clean:poc": "pnpm --filter iterations/poc clean",
    "clean:v2": "pnpm --filter iterations/v2 clean",
    "clean:v3": "pnpm --filter iterations/v3 clean",
    "migrate": "pnpm --filter iterations/poc migrate",
    "migrate:poc": "pnpm --filter iterations/poc migrate",
    "migrate:v2": "pnpm --filter iterations/v2 migrate",
    "migrate:v3": "pnpm --filter iterations/v3 migrate",
    "iterations:list": "node scripts/manage-iterations.js list",
    "iterations:validate": "node scripts/manage-iterations.js validate",
    "iterations:stats": "node scripts/manage-iterations.js stats",
    "iterations:run": "node scripts/manage-iterations.js run",
    "iterations:run-all": "node scripts/manage-iterations.js run-all",
    "caws:validate": "node scripts/caws-validate.js",
    "caws:validate:all": "node scripts/caws-validate.js all",
    "perf:build": "node scripts/benchmark-build-performance.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/agent-agency.git"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "multi-agent",
    "mcp",
    "typescript"
  ],
  "author": "@darianrosebrook",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-org/agent-agency/issues"
  },
  "homepage": "https://github.com/your-org/agent-agency#readme",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "turbo": "^1.12.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
