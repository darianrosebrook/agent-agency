{
  "dashboard": {
    "id": null,
    "title": "Agent Agency V3 - System Overview",
    "tags": ["agent-agency", "v3"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{service}} - {{method}} {{path}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service}}"
          }
        ],
        "yAxes": [
          {
            "unit": "seconds"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "{{service}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percent"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Runtime Optimization Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_agency_runtime_optimization_throughput",
            "legendFormat": "Throughput (ops/sec)"
          },
          {
            "expr": "agent_agency_runtime_optimization_latency_p95",
            "legendFormat": "Latency P95 (ms)"
          },
          {
            "expr": "agent_agency_thermal_throttling_events_total",
            "legendFormat": "Thermal Events"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Federated Learning Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_agency_federation_active_participants",
            "legendFormat": "Active Participants"
          },
          {
            "expr": "agent_agency_privacy_budget_remaining",
            "legendFormat": "Privacy Budget Remaining"
          },
          {
            "expr": "agent_agency_federation_rounds_completed_total",
            "legendFormat": "Rounds Completed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Model Hot-Swap Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_agency_model_hotswap_active_models",
            "legendFormat": "Active Models"
          },
          {
            "expr": "agent_agency_model_hotswap_canary_traffic_percentage",
            "legendFormat": "Canary Traffic %"
          },
          {
            "expr": "agent_agency_model_load_time_seconds",
            "legendFormat": "Model Load Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Tool Ecosystem Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_agency_tool_ecosystem_active_tools",
            "legendFormat": "Active Tools"
          },
          {
            "expr": "agent_agency_tool_executions_total",
            "legendFormat": "Tool Executions"
          },
          {
            "expr": "agent_agency_conflict_resolutions_total",
            "legendFormat": "Conflict Resolutions"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "{{job}} Memory (MB)"
          },
          {
            "expr": "rate(process_cpu_user_seconds_total[5m]) * 100",
            "legendFormat": "{{job}} CPU %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_activity_count{datname=\"agent_agency\"}",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(pg_stat_statements_total_time_seconds[5m])",
            "legendFormat": "Query Time (s)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
